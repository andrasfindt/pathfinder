        <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#start" text="Start" />
        <Box focusTraversable="true" onKeyTyped="#keyReleased" />
        <GridPane hgap="8.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="512.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="74.0" minWidth="10.0" prefWidth="30.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="101.0" minWidth="10.0" prefWidth="44.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="166.0" minWidth="10.0" prefWidth="118.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="90.0" minWidth="10.0" prefWidth="58.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="124.0" minWidth="10.0" prefWidth="53.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="99.0" minWidth="10.0" prefWidth="63.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="145.0" minWidth="10.0" prefWidth="73.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="99.0" minWidth="10.0" prefWidth="19.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Label text="Goal" GridPane.columnIndex="0" />
                <Label text="Gen" GridPane.columnIndex="1" />
                <Label text="Max Fitness" GridPane.columnIndex="2" />
                <Label text="#Steps " GridPane.columnIndex="3" />
                <Label prefWidth="73.0" text="Speed" GridPane.columnIndex="4" />
                <Label prefWidth="66.0" text="PopSize" GridPane.columnIndex="5" />
                <Label prefWidth="66.0" text="M-rate" GridPane.columnIndex="6" />
                <CheckBox fx:id="solved" disable="true" GridPane.columnIndex="0" GridPane.rowIndex="1" />
                <Label fx:id="generation" text="NaN" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <Label fx:id="maxFitness" text="NaN" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                <Label fx:id="stepsTaken" text="NaN" GridPane.columnIndex="3" GridPane.rowIndex="1" />
                <Label fx:id="speed" text="NaN" GridPane.columnIndex="4" GridPane.rowIndex="1" />
                <Label fx:id="popSize" text="NaN" GridPane.columnIndex="5" GridPane.rowIndex="1" />
                <Label fx:id="mutationRate" text="NaN" GridPane.columnIndex="6" GridPane.rowIndex="1" />
                <CheckBox GridPane.columnIndex="7" GridPane.rowIndex="1" onAction="#toggleDrawOld"/>
            </children>
        </GridPane>
    </children>
</AnchorPane>
