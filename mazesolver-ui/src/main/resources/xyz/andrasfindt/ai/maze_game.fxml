<AnchorPane fx:id="gameParent" maxHeight="550.0" maxWidth="512.0" minHeight="550.0" minWidth="512.0" prefHeight="550.0" prefWidth="512.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="xyz.andrasfindt.ai.ui.viewcontroller.MazeGameController">
    <children>
        <Canvas fx:id="gameCanvasBackground" height="512.0" width="512.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
