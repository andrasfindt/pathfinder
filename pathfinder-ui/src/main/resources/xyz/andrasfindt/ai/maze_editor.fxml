<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane maxHeight="572.0" maxWidth="512.0" minHeight="572.0" minWidth="152.0" prefHeight="572.0" prefWidth="512.0"
            xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="xyz.andrasfindt.ai.ui.viewcontroller.MazeEditorController">
    <children>
        <Canvas fx:id="canvas" height="512.0" width="512.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
        <ButtonBar prefHeight="60.0" prefWidth="512.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
            <buttons>
                <Button mnemonicParsing="false" text="Clear" onAction="#clear"/>
                <Button mnemonicParsing="false" text="Export" onAction="#export"/>
                <Button mnemonicParsing="false" text="Load" onAction="#load"/>
                <Button mnemonicParsing="false" text="Use" onAction="#use"/>
            </buttons>
        </ButtonBar>
    </children>
</AnchorPane>
